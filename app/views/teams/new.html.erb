<h1>Create a New Team</h1>
<%= form_for(@team, url: new_user_team_path) do |f| %>
  <p>
	  <%= f.label(:name) %>
	  <%= f.text_field(:name, required: true, autofocus: true) %>
  </p>
  <p>
	  <%= f.label(:description) %>
	  <%= f.text_field(:description required: true) %>
  </p>
  <%= f.submit "Create" %>
<% end %>
